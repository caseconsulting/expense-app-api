{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "debug": "nodemon --trace-warnings --inspect bin/www",
    "eslint": "eslint . --ext .js --ignore-path .jshintignore --fix",
    "lint": "eslint .",
    "pm2": "pm2",
    "pm2:kill": "pm2 kill",
    "pm2:list": "pm2 list",
    "pm2:monit": "pm2 monit",
    "restart": "pm2 restart services.yml",
    "start": "pm2 start services.yml",
    "stop": "pm2 stop services.yml",
    "test": "jasmine || true",
    "test-coverage": "nyc --reporter=text --reporter=html --exclude=spec/** jasmine || true",
    "validate": "npm ls",
    "chronos": "node -e 'require(\"./chronos\").start()'"
  },
  "dependencies": {
    "ajv": "^6.5.2",
    "aws-sdk": "^2.286.2",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.3",
    "debug": "^3.1.0",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-jwt": "^5.3.1",
    "express-jwt-authz": "^1.0.0",
    "jwks-rsa": "^1.3.0",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "moment-range": "^4.0.1",
    "morgan": "^1.9.0",
    "pm2": "^3.0.3",
    "pug": "^2.0.3",
    "serve-favicon": "^2.5.0",
    "through": "^2.3.8",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "aws-sdk-mock": "^4.0.0",
    "cors": "^2.8.4",
    "eslint": "^5.3.0",
    "jasmine": "^3.1.0",
    "nodemon": "^1.18.3",
    "precommit-hook": "^3.0.0"
  },
  "pre-commit": [
    "validate",
    "eslint",
    "test"
  ]
}
