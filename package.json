{
  "name": "expense-app-api",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "chronos": "node -e 'require(\"./chronos\").start()'",
    "claudia": "claudia",
    "debug": "nodemon --trace-warnings --inspect bin/www",
    "deploy:dev": "claudia create --handler lambda.handler --deploy-proxy-api --region us-east-1 --name expense-app-api-dev --runtime nodejs8.10 --set-env VUE_APP_AUTH0_DOMAIN=consultwithcase.auth0.com,VUE_APP_AUTH0_AUDIENCE=https://consultwithcase.auth0.com/api/v2/,STAGE=dev --role expense-api-lambda-dev",
    "destroy": "claudia destroy",
    "eslint": "eslint . --ext .js --ignore-path .jshintignore --fix",
    "lint": "npm run eslint",
    "package:chronos:dev": "aws cloudformation package --template-file CloudFormation.yaml --s3-bucket case-consulting-expense-app-resources-dev --s3-prefix expense-app-api --output-template-file packaged.yaml",
    "pm2": "pm2",
    "pm2:kill": "pm2 kill",
    "pm2:list": "pm2 list",
    "pm2:monit": "pm2 monit",
    "precommit": "npm run eslint && npm run test",
    "restart": "pm2 restart services.yml",
    "start": "pm2 start services.yml",
    "stop": "pm2 stop services.yml",
    "test": "jasmine",
    "test-coverage": "nyc --reporter=text --reporter=html --exclude=spec/** jasmine",
    "update": "claudia update",
    "validate": "npm ls"
  },
  "dependencies": {
    "ajv": "^6.10.2",
    "aws-sdk": "^2.503.0",
    "aws-serverless-express": "^3.3.6",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-jwt": "^5.3.1",
    "express-jwt-authz": "^2.3.1",
    "jwks-rsa": "^1.6.0",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "moment-range": "^4.0.2",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "pm2": "^3.5.1",
    "pug": "^2.0.4",
    "serve-favicon": "^2.5.0",
    "through": "^2.3.8",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "acorn": "^6.2.1",
    "aws-sdk-mock": "^4.5.0",
    "claudia": "^5.6.0",
    "eslint": "^6.1.0",
    "jasmine": "^3.4.0",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1",
    "precommit-hook": "^3.0.0"
  },
  "pre-commit": [
    "eslint",
    "test"
  ]
}
